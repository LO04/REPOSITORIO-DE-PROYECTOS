@model IEnumerable<EventosUPC.Models.Evento_Material>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Evento.nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Material.nombre)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Evento.nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Material.nombre)
        </td>

    </tr>
}

</table>
